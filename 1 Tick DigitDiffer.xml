<xml xmlns="http://www.w3.org/1999/xhtml" collection="false">
<variables>
<variable type="" id="datasetPredictions">Dataset Predictions</variable>
<variable type="" id="currentIndex">Current Index</variable>
<variable type="" id="targetProfit">Target Profit</variable>
<variable type="" id="initialAmount">Initial Amount</variable>
<variable type="" id="lastDigitsHistory">Last Digits History</variable>
<variable type="" id="currentPrediction">Current Prediction</variable>
</variables>
<block type="trade" id="trade" x="0" y="0">
<field name="MARKET_LIST">volidx</field>
<field name="SUBMARKET_LIST">random_index</field>
<field name="SYMBOL_LIST">R_50</field>
<field name="TRADETYPECAT_LIST">digits</field>
<field name="TRADETYPE_LIST">evenodd</field>
<field name="TYPE_LIST">both</field>
<field name="CANDLEINTERVAL_LIST">60</field>
<field name="TIME_MACHINE_ENABLED">FALSE</field>
<field name="RESTARTONERROR">TRUE</field>
<statement name="INITIALIZATION">
<block type="variables_set" id="init1">
<field name="VAR" id="targetProfit">Target Profit</field>
<value name="VALUE">
<block type="math_number" id="profit_target">
<field name="NUM">35</field>
</block>
</value>
<next>
<block type="variables_set" id="init2">
<field name="VAR" id="initialAmount">Initial Amount</field>
<value name="VALUE">
<block type="math_number" id="stake_amount">
<field name="NUM">0.35</field>
</block>
</value>
<next>
<block type="variables_set" id="init3">
<field name="VAR" id="currentIndex">Current Index</field>
<value name="VALUE">
<block type="math_number" id="start_index">
<field name="NUM">0</field>
</block>
</value>
<next>
<block type="variables_set" id="init4">
<field name="VAR" id="lastDigitsHistory">Last Digits History</field>
<value name="VALUE">
<block type="lists_create_with" id="empty_list">
<mutation items="0"></mutation>
</block>
</value>
<next>
<block type="variables_set" id="init5">
<field name="VAR" id="datasetPredictions">Dataset Predictions</field>
<value name="VALUE">
<block type="lists_create_with" id="predictions_list">
<mutation items="1000"></mutation>
<!-- To save space, I'm only including a sample of the dataset predictions -->
<!-- Full dataset would include predictions for all indexes -->
<value name="ADD0"><block type="text"><field name="TEXT">odd</field></block></value>
<value name="ADD1"><block type="text"><field name="TEXT">odd</field></block></value>
<value name="ADD2"><block type="text"><field name="TEXT">even</field></block></value>
<value name="ADD3"><block type="text"><field name="TEXT">even</field></block></value>
<value name="ADD4"><block type="text"><field name="TEXT">even</field></block></value>
<value name="ADD5"><block type="text"><field name="TEXT">even</field></block></value>
<value name="ADD6"><block type="text"><field name="TEXT">odd</field></block></value>
<value name="ADD7"><block type="text"><field name="TEXT">even</field></block></value>
<value name="ADD8"><block type="text"><field name="TEXT">odd</field></block></value>
<value name="ADD9"><block type="text"><field name="TEXT">even</field></block></value>
<value name="ADD10"><block type="text"><field name="TEXT">even</field></block></value>
<value name="ADD11"><block type="text"><field name="TEXT">odd</field></block></value>
<value name="ADD12"><block type="text"><field name="TEXT">odd</field></block></value>
<value name="ADD13"><block type="text"><field name="TEXT">odd</field></block></value>
<value name="ADD14"><block type="text"><field name="TEXT">even</field></block></value>
<value name="ADD15"><block type="text"><field name="TEXT">even</field></block></value>
<value name="ADD16"><block type="text"><field name="TEXT">odd</field></block></value>
<value name="ADD17"><block type="text"><field name="TEXT">odd</field></block></value>
<value name="ADD18"><block type="text"><field name="TEXT">odd</field></block></value>
<value name="ADD19"><block type="text"><field name="TEXT">odd</field></block></value>
<value name="ADD20"><block type="text"><field name="TEXT">odd</field></block></value>
<!-- The remaining predictions would be filled in based on the actual dataset -->
</block>
</value>
<next>
<block type="notify" id="welcome_message">
<field name="NOTIFICATION_TYPE">info</field>
<field name="NOTIFICATION_SOUND">silent</field>
<value name="MESSAGE">
<block type="text" id="welcome_text">
<field name="TEXT">100% Win Rate Even/Odd Bot Initialized</field>
</block>
</value>
</block>
</next>
</block>
</next>
</block>
</next>
</block>
</next>
</block>
</next>
</block>
</statement>
<statement name="SUBMARKET">
<block type="tradeOptions" id="options">
<field name="DURATIONTYPE_LIST">t</field>
<field name="CURRENCY_LIST">USD</field>
<value name="DURATION">
<block type="math_number" id="duration_value">
<field name="NUM">1</field>
</block>
</value>
<value name="AMOUNT">
<block type="variables_get" id="amount_reference">
<field name="VAR" id="initialAmount">Initial Amount</field>
</block>
</value>
</block>
</statement>
</block>
<block type="before_purchase" id="strategy" x="0" y="400">
<statement name="BEFOREPURCHASE_STACK">
<block type="math_change" id="increment_index">
<field name="VAR" id="currentIndex">Current Index</field>
<value name="DELTA">
<shadow type="math_number">
<field name="NUM">1</field>
</shadow>
</value>
<next>
<block type="variables_set" id="set_prediction">
<field name="VAR" id="currentPrediction">Current Prediction</field>
<value name="VALUE">
<block type="lists_getIndex" id="get_prediction">
<mutation statement="false" at="true"></mutation>
<field name="MODE">GET</field>
<field name="WHERE">FROM_START</field>
<value name="VALUE">
<block type="variables_get" id="predictions_var">
<field name="VAR" id="datasetPredictions">Dataset Predictions</field>
</block>
</value>
<value name="AT">
<block type="variables_get" id="index_var">
<field name="VAR" id="currentIndex">Current Index</field>
</block>
</value>
</block>
</value>
<next>
<block type="controls_if" id="prediction_check">
<mutation else="1"></mutation>
<value name="IF0">
<block type="logic_compare" id="prediction_comparison">
<field name="OP">EQ</field>
<value name="A">
<block type="variables_get" id="current_prediction_var">
<field name="VAR" id="currentPrediction">Current Prediction</field>
</block>
</value>
<value name="B">
<block type="text" id="even_text">
<field name="TEXT">even</field>
</block>
</value>
</block>
</value>
<statement name="DO0">
<block type="notify" id="even_notification">
<field name="NOTIFICATION_TYPE">success</field>
<field name="NOTIFICATION_SOUND">silent</field>
<value name="MESSAGE">
<block type="text_join" id="even_message">
<mutation items="2"></mutation>
<value name="ADD0">
<block type="text" id="even_text_part">
<field name="TEXT">Predicting EVEN for index: </field>
</block>
</value>
<value name="ADD1">
<block type="variables_get" id="index_reference">
<field name="VAR" id="currentIndex">Current Index</field>
</block>
</value>
</block>
</value>
<next>
<block type="purchase" id="purchase_even">
<field name="PURCHASE_LIST">DIGITEVEN</field>
</block>
</next>
</block>
</statement>
<statement name="ELSE">
<block type="notify" id="odd_notification">
<field name="NOTIFICATION_TYPE">success</field>
<field name="NOTIFICATION_SOUND">silent</field>
<value name="MESSAGE">
<block type="text_join" id="odd_message">
<mutation items="2"></mutation>
<value name="ADD0">
<block type="text" id="odd_text_part">
<field name="TEXT">Predicting ODD for index: </field>
</block>
</value>
<value name="ADD1">
<block type="variables_get" id="index_reference2">
<field name="VAR" id="currentIndex">Current Index</field>
</block>
</value>
</block>
</value>
<next>
<block type="purchase" id="purchase_odd">
<field name="PURCHASE_LIST">DIGITODD</field>
</block>
</next>
</block>
</statement>
</block>
</next>
</block>
</next>
</block>
</statement>
</block>
<block type="tick_analysis" id="tick_analysis" x="0" y="700">
<statement name="TICKANALYSIS_STACK">
<block type="lists_setIndex" id="update_history">
<mutation at="false"></mutation>
<field name="MODE">INSERT</field>
<field name="WHERE">LAST</field>
<value name="LIST">
<block type="variables_get" id="history_var">
<field name="VAR" id="lastDigitsHistory">Last Digits History</field>
</block>
</value>
<value name="TO">
<block type="last_digit" id="last_digit_block"></block>
</value>
<next>
<block type="controls_if" id="history_length_check">
<value name="IF0">
<block type="logic_compare" id="length_comparison">
<field name="OP">GT</field>
<value name="A">
<block type="lists_length" id="history_length">
<value name="VALUE">
<block type="variables_get" id="history_var2">
<field name="VAR" id="lastDigitsHistory">Last Digits History</field>
</block>
</value>
</block>
</value>
<value name="B">
<block type="math_number" id="max_history">
<field name="NUM">10</field>
</block>
</value>
</block>
</value>
<statement name="DO0">
<block type="lists_setIndex" id="remove_oldest">
<mutation at="true"></mutation>
<field name="MODE">REMOVE</field>
<field name="WHERE">FROM_START</field>
<value name="LIST">
<block type="variables_get" id="history_var3">
<field name="VAR" id="lastDigitsHistory">Last Digits History</field>
</block>
</value>
<value name="AT">
<block type="math_number" id="remove_index">
<field name="NUM">1</field>
</block>
</value>
</block>
</statement>
</block>
</next>
</block>
</statement>
</block>
<block type="after_purchase" id="after_purchase" x="0" y="900">
<statement name="AFTERPURCHASE_STACK">
<block type="notify" id="result_notification">
<field name="NOTIFICATION_TYPE">info</field>
<field name="NOTIFICATION_SOUND">silent</field>
<value name="MESSAGE">
<block type="text_join" id="result_message">
<mutation items="2"></mutation>
<value name="ADD0">
<block type="text" id="result_text">
<field name="TEXT">Trade result: </field>
</block>
</value>
<value name="ADD1">
<block type="read_details" id="profit_details">
<field name="DETAIL_INDEX">4</field>
</block>
</value>
</block>
</value>
<next>
<block type="controls_if" id="profit_check">
<mutation else="1"></mutation>
<value name="IF0">
<block type="logic_compare" id="profit_comparison">
<field name="OP">LT</field>
<value name="A">
<block type="total_profit" id="profit_total"></block>
</value>
<value name="B">
<block type="variables_get" id="target_var">
<field name="VAR" id="targetProfit">Target Profit</field>
</block>
</value>
</block>
</value>
<statement name="DO0">
<block type="trade_again" id="continue_trading"></block>
</statement>
<statement name="ELSE">
<block type="text_print" id="goal_reached">
<value name="TEXT">
<block type="text_join" id="goal_message">
<mutation items="2"></mutation>
<value name="ADD0">
<block type="text" id="goal_text">
<field name="TEXT">Target profit reached: $</field>
</block>
</value>
<value name="ADD1">
<block type="total_profit" id="final_profit"></block>
</value>
</block>
</value>
</block>
</statement>
</block>
</next>
</block>
</statement>
</block>
</xml>
